{
  "name": "paper-owl",
  "displayName": "Paper Owl",
  "description": "An adaptation of Steph Ango's Flexoki color scheme https://stephango.com/flexoki",
  "version": "2.0.1",
  "publisher": "third774",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/third774/paper-owl-vscode-theme"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Light Theme",
    "Paper Owl",
    "Night Theme",
    "Early Bird",
    "Accessible Theme"
  ],
  "scripts": {
    "publish": "vsce publish"
  },
  "galleryBanner": {
    "color": "#100F0F",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "owl-icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Paper Owl",
        "uiTheme": "vs-dark",
        "path": "./themes/Paper Owl-color-theme.json"
      },
      {
        "label": "Paper Owl (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/Paper Owl-color-theme-noitalic.json"
      },
      {
        "label": "Paper Owl Light",
        "uiTheme": "vs",
        "path": "./themes/Paper Owl-Light-color-theme.json"
      },
      {
        "label": "Paper Owl Light (No Italics)",
        "uiTheme": "vs",
        "path": "./themes/Paper Owl-Light-color-theme-noitalic.json"
      }
    ]
  }
}
